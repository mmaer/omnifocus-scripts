{
  "name": "omnifocus-scripts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && npm run build:linking && npm run build:updateDatesAndTime && npm run build:estimateTime",
    "build:linking": "SCRIPT=linking vite build",
    "build:updateDatesAndTime": "SCRIPT=updateDatesAndTime vite build",
    "build:estimateTime": "SCRIPT=estimateTime vite build",
    "copy-scripts": "bash copy-scripts.sh",
    "buildAndCopy": "npm run build && npm run copy-scripts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "dependencies": {
    "vite-tsconfig-paths": "^4.3.2"
  }
}
